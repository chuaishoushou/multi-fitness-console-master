<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.CardCategoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.CardCategory">
        <id column="id" property="id" />
        <result column="club_id" property="clubId" />
        <result column="title" property="title" />
        <result column="expireTime" property="expireTime" />
        <result column="expireDay" property="expireDay" />
        <result column="value" property="value" />
        <result column="leaveMaxDay" property="leaveMaxDay" />
        <result column="enabled" property="enabled" />
        <result column="shareRestrict" property="shareRestrict" />
        <result column="card_type" property="cardType" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, club_id AS clubId, title, expireTime, expireDay, value, leaveMaxDay, enabled, shareRestrict, card_type AS cardType
    </sql>

</mapper>
