view {
	box-sizing: border-box;
}
.h1,
.h2,
.h3 {
	line-height: 2;
	font-weight: normal
}

.h1 {
	font-size: 180rpx
}

.h2 {
	font-size: 160rpx
}

.h3 {
	font-size: 140rpx
}


.top-border:before {
	content: " ";
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 2rpx;
	border-top: 2rpx solid #dedede;
	color: #dedede;
	-webkit-transform-origin: 0 0;
	transform-origin: 0 0;
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5)
}

.bottom-border:after {
	content: " ";
	position: absolute;
	left: 0;
	bottom: 0;
	width: 100%;
	height: 2rpx;
	border-bottom: 2rpx solid #dedede;
	color: #dedede;
	-webkit-transform-origin: 0 100%;
	transform-origin: 0 100%;
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5)
}

.header-webkit {
	height: 358rpx;
	background-color: #929292;
	background-repeat: no-repeat;
	background-size: cover;
	background-position: center top;
	text-align: center;
	overflow: hidden;
	position: relative
}

.header-webkit .pclub {
	position: absolute;
	top: 10rpx;
	right: 10rpx;
	color: #fff;
	font-size: 30rpx
}

.header-webkit .club-logo {
	background: #ccc;
	border-radius: 8rpx;
	border: 4rpx solid #fff;
	width: 100rpx;
	height: 100rpx;
	box-sizing: border-box;
	margin: 49rpx auto 0 auto
}

.header-webkit .club-logo image {
	max-width: 100%;
	width: 100%;
	height: 100%
}

.header-webkit .club-name {
	height: 44rpx;
	line-height: 44rpx;
	font-size: 34rpx;
	color: #fff;
	margin-top: 17rpx;
	overflow: hidden;
	text-overflow: ellipsis;
	white-space: nowrap
}

.header-webkit .club-describe {
	height: 31rpx;
	line-height: 31rpx;
	font-size: 22rpx;
	color: #fff
}

.header-webkit .header-but {
	position: absolute;
	left: 0;
	right: 0;
	bottom: 0;
	height: 98rpx
}

.header-webkit .header-but .but-webkit {
	font-size: 0;
	line-height: 1
}

.header-webkit .header-but .but-webkit .icon {
	max-width: 100%;
	width: 60rpx;
	height: 60rpx;
	vertical-align: top;
	margin: 19rpx 20rpx 0 0
}

.header-webkit .header-but .but-webkit .text {
	font-size: 32rpx;
	color: #fff;
	height: 100%;
	line-height: 98rpx;
	display: inline-block
}

.header-webkit .header-but .but-webkit:active {
	opacity: 0.6
}

.header-webkit .header-but .but-webkit:nth-child(1) {
	position: relative
}

.header-webkit .header-but .but-webkit:nth-child(1):after {
	content: '';
	position: absolute;
	box-sizing: border-box;
	width: 1rpx;
	height: 80rpx;
	right: 0;
	top: 30rpx;
	border-left: 1px solid rgba(255, 255, 255, 0.3);
	color: rgba(255, 255, 255, 0.3);
	background: transparent;
	-webkit-transform-origin: 0 0;
	transform-origin: 0 0;
	-webkit-transform: scale(.5);
	transform: scale(.5)
}
  
