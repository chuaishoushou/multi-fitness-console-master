}
.stat-content .item{
    margin: 16rpx 0;
    padding: 0 20rpx;
		vertical-align:top;
}
.stat-content .item .num{
	margin: 16rpx 0 4rpx;
	font-size: 50rpx;
}

.icon-content{
  margin: 16rpx 0;
  text-align: center;
  flex-wrap:wrap;
  padding:26rpx 0 0;
}

.icon-content .item{
	width:33.33%;
  margin: 20rpx 0;
  padding: 0 20rpx;
	vertical-align:top;
}

.icon-content .item .icon {
	width:96rpx;
	height:96rpx;
}

.icon-content .item .name {
	font-size:25rpx;
	height:48rpx;
	line-height:48rpx;
	overflow:hidden;
	text-overflow:ellipsis;
