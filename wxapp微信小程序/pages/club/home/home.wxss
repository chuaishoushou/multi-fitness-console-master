	margin-top: 40rpx
}

.pup-eqr-webkit .pup-eqr .top .userQrcode-cnvent {
	margin-top: 0.5*100rpx;
	margin-bottom: 0.3*100rpx
}

.pup-eqr-webkit .pup-eqr .top .userQrcode-cnvent image {
	max-width: 100%;
	width: 4.5*100rpx;
	height: 4.5*100rpx;
	overflow: hidden
}

.pup-eqr-webkit .pup-eqr .bottom-but {
	height: 1*100rpx;
	position: relative;
	background: #f7f7f7
}

.pup-eqr-webkit .pup-eqr .bottom-but:before {
	content: " ";
	position: absolute;
	left: 0;
	top: 0;
	width: 100%;
	height: 1px;
	border-top: 1px solid #dedede;
	color: #dedede;
	-webkit-transform-origin: 0 0;
	transform-origin: 0 0;
	-webkit-transform: scaleY(.5);
	transform: scaleY(.5)
}

.pup-eqr-webkit .pup-eqr .bottom-but navigator {
	position: relative;
	color: #808080;
	font-size: 0.24*100rpx;
	line-height: 1.1*100rpx
}

.pup-eqr-webkit .pup-eqr .bottom-but navigator:before {
	content: " ";
	position: absolute;
	right: 0;
	top: 0;
	width: 1px;
	height: 100%;
	border-right: 1px solid #dedede;
	color: #dedede;
	-webkit-transform-origin: 100% 0;
	transform-origin: 100% 0;
	-webkit-transform: scaleX(.5);
	transform: scaleX(.5)
}

.pup-eqr-webkit .pup-eqr .bottom-but navigator:active {
	background: #ebebeb
}

.pup-eqr-webkit .pup-eqr .bottom-but navigator image {
	max-width: 100%;
	width: 0.44*100rpx;
	height: 0.44*100rpx;
	position: relative;
	top: -0.04*100rpx;
	margin-left: 0.03*100rpx
}

.pup-eqr-webkit .pup-eqr .bottom-but .switch-club:before {
